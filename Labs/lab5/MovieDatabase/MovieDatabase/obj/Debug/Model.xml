		<Element Type="SqlProcedure" Name="[dbo].[UpdateMovie]">
			<Property Name="BodyScript">
				<Value><![CDATA[ BEGIN
	SET NOCOUNT ON;

	IF NOT EXISTS (SELECT * FROM Movies WHERE Id = @id)
	BEGIN
		RAISERROR('Movie not found', 16, 1)
		RETURN
	END

	UPDATE Movies
	SET 
		Title = @title, 
		Description = @description, 
		Length = @length, 
		IsOwned = @isOwned
	WHERE Id = @id
END]]></Value>
